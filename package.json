{
  "name": "he-react-ui",
  "version": "0.1.53",
  "description": "Common UI elements for HealthEngine",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "yarn run clean && webpack --config ./config/webpack.prod.js -p",
    "test": "NODE_ENV=test jest --env=jsdom",
    "coverage": "NODE_ENV=test jest --env=jsdom --coverage",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "test:watch": "yarn test -- --watch",
    "lint": "eslint . --env jest",
    "lint-fix": "eslint . --fix --env jest",
    "cosmos": "cosmos",
    "precommit": "yarn verify",
    "verify": "yarn lint && yarn flow && yarn coverage && yarn jest-coverage-ratchet"
  },
  "keywords": [
    "HealthEngine",
    "react",
    "UI",
    "components"
  ],
  "author": "HealthEngine",
  "license": "MIT",
  "dependencies": {
    "babel-preset-flow": "^6.23.0",
    "classnames": "^2.2.5",
    "coveralls": "^3.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "npm:enzyme-react-adapter-future",
    "enzyme-to-json": "^3.3.4",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-he": "^0.2.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.48.0",
    "eslint-plugin-import-order-autofix": "^0.8.3",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "flow-bin": "^0.75.0",
    "is-absolute-url": "^2.1.0",
    "is-url-external": "^1.0.3",
    "jest": "^22.4.3",
    "jest-coverage-ratchet": "^0.2.3",
    "moment": "^2.22.1",
    "nwsapi": "^2.0.4",
    "object-assign": "^4.1.1",
    "prettier": "^1.13.2",
    "promise": "^8.0.1",
    "psl": "^1.1.28",
    "raf": "^3.4.0",
    "react": "^16.3.0",
    "react-cosmos": "^4.5.0",
    "react-cosmos-router-proxy": "^4.2.1",
    "react-cosmos-telescope": "^4.4.0",
    "react-cosmos-test": "^4.4.0",
    "react-dates": "^16.6.1",
    "react-dom": "^16.3.0",
    "react-loadable": "^5.3.1",
    "react-media": "^1.8.0",
    "react-onclickoutside": "^6.7.1",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-select": "^1.2.1",
    "react-tippy": "^1.2.2",
    "react-youtube": "^7.6.0",
    "url-loader": "^1.0.1",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "autoprefixer": "^8.0.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.1.2",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "css-loader": "^0.28.9",
    "eslint": "^4.18.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.6.1",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "identity-obj-proxy": "^3.0.0",
    "node-sass": "^4.7.2",
    "postcss-flexbugs-fixes": "^3.3.0",
    "postcss-loader": "^2.1.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.1",
    "webpack": "^3.10.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/*.fixture.js"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 85.81,
        "statements": 85.81,
        "functions": 75.61,
        "branches": 81.98
      }
    },
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "json-summary"
    ],
    "setupFiles": [
      "<rootDir>/config/jest.config.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(test).js?(x)"
    ],
    "testEnvironment": "node",
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  }
}
