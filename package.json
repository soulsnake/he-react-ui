{
  "name": "he-react-ui",
  "version": "2.2.0",
  "description": "Common UI elements for HealthEngine",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "yarn run clean && webpack --config ./config/webpack.prod.js -p",
    "test": "NODE_ENV=test jest --env=jsdom",
    "coverage": "NODE_ENV=test jest --env=jsdom --coverage",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "test:watch": "yarn test -- --watch",
    "lint": "eslint . --env jest --ext js,ts,tsx",
    "lint-fix": "eslint . --fix --env jest",
    "cosmos": "cosmos",
    "cosmos-export": "cross-env COSMOS_PUBLIC_URL='./' cosmos-export",
    "precommit": "yarn && yarn lint --fix && yarn coverage && yarn ratchet-coverage",
    "ratchet-coverage": "node config/ratchetCoverage.js",
    "verify": "yarn && yarn lint && cross-env CI=1 yarn coverage && yarn ratchet-coverage"
  },
  "keywords": [
    "HealthEngine",
    "react",
    "UI",
    "components"
  ],
  "author": "HealthEngine",
  "license": "MIT",
  "dependencies": {
  },
  "devDependencies": {
    "@types/classnames": "^2.2.6",
    "@types/enzyme": "^3.1.14",
    "@types/is-absolute-url": "^2.1.0",
    "@types/jest": "^23.3.3",
    "@types/moment": "^2.13.0",
    "@types/react": "^16.4.14",
    "@types/react-dates": "^17.1.2",
    "@types/react-onclickoutside": "^6.0.4",
    "@types/react-router": "^4.0.31",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-select": "^1.2.1",
    "@types/react-youtube": "^7.4.2",
    "autoprefixer": "^8.0.0",
    "classnames": "^2.2.5",
    "concurrently": "^3.6.1",
    "coveralls": "^3.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "enzyme": "^3.3.0",
    "enzyme-react-adapter-future": "^1.1.3",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^4.18.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.6.1",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-typescript": "^0.12.0",
    "event-source-polyfill": "^1.0.3",
    "har-validator": "^5.1.3",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "is-absolute-url": "^2.1.0",
    "is-url-external": "^1.0.3",
    "jest": "^22.4.3",
    "moment": "^2.22.1",
    "node-sass": "^4.10.0",
    "nwsapi": "^2.0.4",
    "object-assign": "^4.1.1",
    "postcss-flexbugs-fixes": "^3.3.0",
    "postcss-loader": "^2.1.1",
    "prettier": "^1.13.2",
    "promise": "^8.0.2",
    "psl": "^1.1.28",
    "raf": "^3.4.0",
    "react": "^16.3.0",
    "react-cosmos": "^4.6.4",
    "react-cosmos-router-proxy": "^4.6.1",
    "react-cosmos-telescope": "^4.6.4",
    "react-cosmos-test": "^4.6.4",
    "react-dates": "^16.6.1",
    "react-dom": "^16.3.0",
    "react-loadable": "^5.3.1",
    "react-media": "^1.8.0",
    "react-onclickoutside": "^6.7.1",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-select": "^1.2.1",
    "react-tippy": "^1.2.2",
    "react-with-direction": "^1.3.0",
    "react-youtube": "^7.6.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.1",
    "ts-jest": "^23.10.3",
    "ts-loader": "^5.2.1",
    "typescript": "3.0.1",
    "typescript-eslint-parser": "^19.0.2",
    "unfetch": "^4.0.1",
    "url-loader": "^1.0.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "whatwg-fetch": "^2.0.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.fixture{s,}.*",
      "!src/**/fixtures/*.*",
      "!src/index.js"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "json-summary"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/config/jest.config.tsx",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.ts?(x)",
      "<rootDir>/src/**/?(*.)(test).ts?(x)"
    ],
    "testEnvironment": "node",
    "modulePaths": [
      "<rootDir>/"
    ],
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/fileTransform.js"
    },
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "ts",
      "tsx"
    ]
  }
}
